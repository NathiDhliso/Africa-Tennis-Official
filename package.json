{
  "name": "africa-tennis-platform",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "lint:strict": "eslint . --max-warnings 0",
    "preview": "vite preview",
    "deploy:backend": "cd aws && npm run build && npm run deploy",
    "deploy:frontend": "npm run build && echo 'Frontend built successfully'",
    "deploy:aws": "cd aws && sam build && sam deploy --guided",
    "test:backend": "cd aws && npm test",
    "dev:aws": "cross-env VITE_USE_AWS=true npm run dev",
    "build:aws": "cross-env VITE_USE_AWS=true npm run build",
    "dev:aws:win": "set VITE_USE_AWS=true && npm run dev",
    "build:aws:win": "set VITE_USE_AWS=true && npm run build",
    "test": "cypress open",
    "test:headless": "cypress run",
    "add-test-users": "tsx src/scripts/add-test-users.ts",
    "analyze": "vite build --mode analyze",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "prepare": "husky || echo 'Husky not available, skipping...'"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@sentry/react": "^7.99.0",
    "@supabase/supabase-js": "^2.39.3",
    "@tanstack/react-query": "^5.17.0",
    "@tanstack/react-query-devtools": "^5.17.0",
    "long": "^5.2.3",
    "lucide-react": "^0.344.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.12",
    "react-hook-form": "^7.49.3",
    "react-intersection-observer": "^9.8.1",
    "react-router-dom": "^6.26.0",
    "react-webcam": "^7.2.0",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/node": "^22.0.0",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "cypress": "^13.6.6",
    "eslint": "^9.0.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "globals": "^15.0.0",
    "husky": "^9.0.0",
    "postcss": "^8.4.35",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vite-plugin-compression": "^0.5.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
